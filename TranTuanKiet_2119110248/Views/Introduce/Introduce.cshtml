@model TranTuanKiet_2119110248.Models.HomeModel
@{
    ViewBag.Title = "Introduce";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h2> Giới Thiệu E-commerce</h2>
            @foreach (var item in Model.lstabouts)
            {
                <img src="~/Content/images/AboutUs/anh1.jpg" />
                <p>@item.Introduct</p>
            }
        </div>
        <div class="col-md-6">
            <h2>Chính Sách Của Công Ty</h2>
            @foreach (var item in Model.lstabouts)
            {
                <div>@item.policy</div>
            }
        </div>
    </div>
</div>

